{
  "expo": {
    "plugins": [
      [
        "./plugins/android-manifest-fileprovider-definitions.js"
      ],
      [
        "./plugins/jvm-memory.js"
      ],
      [
        "expo-image-picker",
        {
          "photosPermission": "The app accesses your photos to let you share them with your contacts in private chats."
        }
      ],
      [
        "expo-build-properties",
        {
          "android": {
            "compileSdkVersion": 33,
            "targetSdkVersion": 34,
            "usesCleartextTraffic": true,
            "packagingOptions": {
              "pickFirst": [
                "**/libcrypto.so"
              ]
            }
          },
          "ios": {
            "deploymentTarget": "16.0"
          }
        }
      ]
    ],
    "name": "enhmessageme",
    "slug": "enhmessageme",
    "scheme": "enhmsm",
    "version": "2.3.66",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "pt.lasige.safex.enhmessageme",
      "infoPlist": {
        "LSApplicationQueriesSchemes": [
          "pripro"
        ],
        "NSCameraUsageDescription": "This app uses the camera to let users share images in private chats.",
        "NSAppTransportSecurity": {
          "NSAllowsArbitraryLoads": true
        }
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "permissions": [
        "CAMERA",
        "WRITE_EXTERNAL_STORAGE",
        "CAMERA_ROLL"
      ],
      "package": "pt.lasige.safex.enhmessageme",
      "versionCode": 205
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "ca35569b-ed5a-4669-8487-7fbf6bef1b84"
      }
    }
  }
}
